# Discord Webhook URL
# Get this from Discord: Server Settings > Integrations > Webhooks > New Webhook
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Project Updates Webhook URL (for deadline alerts)
# Create a webhook for your #project-updates channel
PROJECT_UPDATES_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_PROJECT_WEBHOOK_ID/YOUR_PROJECT_WEBHOOK_TOKEN

# Alert Check Time (Cron format: minute hour day month weekday)
# Default: 0 9 * * * (9:00 AM daily)
ALERT_CHECK_TIME=0 9 * * *

# Server Port
PORT=3000
